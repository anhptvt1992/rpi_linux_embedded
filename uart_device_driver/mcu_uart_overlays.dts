/dts-v1/;
/plugin/;

/ {
	
	compatible = "brcm,bcm2837";
	
	#address-cells = <1>;
	#size-cells = <0>;

	fragment@0 {
		
		reg = <0>;

		target = <&uart1>;
		
		__overlay__{

			status = "okay";

			#address-cells = <1>;
			#size-cells = <0>;

			uart_mcu@0 {

				compatible = "uart, mcu_stm32";
				
			};
		};
	};	
};